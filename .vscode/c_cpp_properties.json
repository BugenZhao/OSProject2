{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "~/Android/android-ndk-linux/toolchains/arm-linux-androideabi-4.9/prebuilt/linux-x86_64/lib/gcc/arm-linux-androideabi/4.9/include",
                "~/Android/kernel/goldfish/include/**",
                "~/Android/kernel/goldfish/include/config/**",
                "~/Android/kernel/goldfish/mm",
                "~/Android/kernel/goldfish/arch/arm/include/**",
                "~/Android/kernel/goldfish/arch/arm/mach-goldfish/include",
                "~/Android/kernel/goldfish/include/linux/kconfig.h",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "__KERNEL__",
                "__GNUC__=4",
                "_GNU_SOURCE",
                "__LINUX_ARM_ARCH__=7",
                "CONFIG_HZ=100",
                "CONFIG_MMU"
            ],
            "compilerArgs": [
                "-nostdinc",
                "-fno-strict-aliasing",
                "-fno-common",
                "-fno-pic"
            ],
            "compilerPath": "~/Android/android-ndk-linux/toolchains/arm-linux-androideabi-4.9/prebuilt/linux-x86_64/bin/arm-linux-androideabi-gcc",
            "cStandard": "gnu89",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "~/Android/kernel/goldfish/compile_commands.json",
            "browse": {
                "path": [
                    "~/Android/kernel/goldfish/include/**",
                    "~/Android/kernel/goldfish/arch/arm/include/**",
                    "~/Android/kernel/goldfish/include/linux/kconfig.h",
                    "${workspaceFolder}/**",
                    "${default}"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}
